<h1>Listing student_districts</h1>

<table>
  <tr>
    <th>District</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @student_districts.each do |student_district| %>
  <tr>
    <td><%= student_district.district %></td>
    <td><%= link_to 'Show', student_district %></td>
    <td><%= link_to 'Edit', edit_student_district_path(student_district) %></td>
    <td><%= link_to 'Destroy', student_district, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

