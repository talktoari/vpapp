<!DOCTYPE html>
<html>
<head>
  <title>Vpapp</title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "stylesheets/default.css" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <!-- Common Menu for all pages of the application -->
  <%= render "shared/menu" %>

<!-- Common SignIn or SignOut for all pages of the application -->
<%= render "shared/signin_signout" %>

<%  if notice  %>
  <p class="notice"><%= notice %></p>
<% end %>
<%  if alert  %>
  <p class="alert"><%= alert %></p>
<% end %>

<%= yield %>

</body>
</html>

