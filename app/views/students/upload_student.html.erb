<fieldset>
<legend>Upload Student</legend>
	<h3> Choose Upload file</h3>
	<h4>Uploads, Parses, Validates and stores the Students Data in DB</h4>
	<%= form_tag('/students/upload_validate_student', :multipart => true) %>
		<p>
			<%= file_field_tag 'excel_file' %>
		</p>
		<p>
			<%= submit_tag "Start Upload" %>
		</p>
	</form>
</fieldset>