<% unless @errors.empty? %>
<h1>Failure Entries</h1>
				<ul>
					<% @errors.each do |error| %>
					<li>
						Problems in line number <%= error[0] %>:
						<% error[1].full_messages.each do |msg|%>
						<p><%= msg %> </p>
						<%end%>
					</li>
					<% end %>
				</ul>
<% else %>
	<h1>ALL SUCCESS</h1>
<% end %>
<h1>Success Entries</h1>
<table id="donations">
  <tr>
  	<th>Student ID</th>
    <th>Year</th>
    <th>College Name and Address</th>
    <th>College Phone</th>
    <th>Course</th>
    <th>Stream</th>
    <th>Amount Approved</th>
    <th>Books Given</th>
    <th>Facilitator Name and Address</th>
    <th>Letter1 Sent</th>
    <th>Letter2 Sent</th>
  </tr>
<% student = Student.new %>
<% @yearly_details.each do |yearly_detail| %>
  <tr>
  	<% student = yearly_detail.student %>
  	<td><%= yearly_detail.student_id %></td>
    <td><%= yearly_detail.year %></td>
    <td><%= yearly_detail.coll_full_address %></td>
    <td><%= yearly_detail.coll_primary_phone %></td>
    <td><%= yearly_detail.course %></td>
    <td><%= yearly_detail.stream %></td>
    <td><%=	yearly_detail.amount_total %></td>
    <td><%= yearly_detail.books_given %></td>
    <td><%= yearly_detail.fac_full_address %></td>
    <td><%= yearly_detail.letter1_sent %></td>
    <td><%= yearly_detail.letter2_sent %></td>
  </tr>
<% end %>
</table>
<h4>Back to Student <%= link_to "#{student.first_name} #{student.last_name}", student %></h4>