<fieldset>
<legend>Upload Yearly Detail</legend>
	<h3> Choose Upload file</h3>
	<h4>Uploads, Parses, Validates and stores the Yearly Data for Student</h4>
	<%= form_tag('/yearly_details/upload_validate_yeardata', :multipart => true) %>
		<%= hidden_field_tag 'student_id', params[:student_id] %>
		<p>
			<%= file_field_tag 'excel_file' %>
		</p>
		<p>
			<%= submit_tag "Start Upload" %>
		</p>
	</form>
</fieldset>